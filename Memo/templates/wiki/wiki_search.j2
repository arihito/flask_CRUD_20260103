{% extends "base.j2" %}
{% block content %}
  <h1>Wikipediaの検索</h1>
  {% from '_formhelpers.j2' import render_field %}
  <form action="{{ url_for('wiki.search') }}" method="post" novalidate>
    {{ form.hidden_tag() }}
    {{ render_field(form.keyword) }}
    {{ form.submit }}
  </form>
  <p>
    <a href="{{ url_for('memo.index') }}">一覧ページへ戻る</a>
  </p>
{% endblock content %}
